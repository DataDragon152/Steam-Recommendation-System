ormer(\n","      (layer): ModuleList(\n","        (0-5): 6 x TransformerBlock(\n","          (attention): DistilBertSdpaAttention(\n","            (dropout): Dropout(p=0.1, inplace=False)\n","            (q_lin): Linear(in_features=768, out_features=768, bias=True)\n","            (k_lin): Linear(in_features=768, out_features=768, bias=True)\n","            (v_lin): Linear(in_features=768, out_features=768, bias=True)\n","            (out_lin): Linear(in_features=768, out_features=768, bias=True)\n","          )\n","          (sa_layer_norm): LayerNorm((768,), eps=1e-12, elementwise_affine=True)\n","          (ffn): FFN(\n","            (dropout): Dropout(p=0.1, inplace=False)\n","            (lin1): Linear(in_features=768, out_features=3072, bias=True)\n","            (lin2): Linear(in_features=3072, out_features=768, bias=True)\n","            (activation): GELUActivation()\n","          )\n","          (output_layer_norm): LayerNorm((768,), eps=1e-12, elementwise_affine=True)\n","        )\n","      )\n","    )\n","  )\n","  (pre_classifier): Linear(in_features=768, out_features=768, bias=True)\n","  (classifier): Linear(in_features=768, out_features=2, bias=True)\n","  (dropout): Dropout(p=0.2, inplace=False)\n",")"]},"metadata":{},"execution_count":9}]},{"cell_type":"code","source":["# 3. Load dữ liệu\n","texts = df[\"review_clean\"].astype(str).str.strip().str.replace('\\n', ' ').str.lower().tolist()"],"metadata":{"id":"EVzckSBf5VQb","executionInfo":{"status":"ok","timestamp":1745911692376,"user_tz":-420,"elapsed":1900,"user":{"displayName":"Long Nguyen","userId":"14589141717299064407"}}},"execution_count":10,"outputs":[]},{"cell_type":"code","source":["def predict_sentiment_batch(texts, batch_size=32):  # giảm batch_size\n","    results = []\n","\n","    model.eval()  # Đảm bảo chế độ eval\n","    texts = [t for t in texts if isinstance(t, str) and t.strip() != \"\"]  # bỏ rỗng\n","\n","    for i in tqdm(range(0, len(texts), batch_size), desc=\"Predicting Sentiment\"):\n","        batch_texts = texts[i:i+batch_size]\n","        try:\n","            inputs = tokenizer.batch_encode_plus(\n","                batch_texts,\n","                return_tensors=\"pt\",\n","                padding=True,\n","                truncation=True,\n","                max_length=128\n","            )\n","            inputs = {key: val.to(device) for key, val in inputs.items()}\n","\n","            with torch.no_grad():  # Tắt tính gradient → nhanh hơn nhiều\n","                logits = model(**inputs).logits\n","\n","            probs = torch.nn.functional.softmax(logits, dim=1).cpu().numpy()\n","            labels = np.argmax(probs, axis=1)\n","\n","            results.extend([\n","                {\n","                    \"label\": \"positive\" if label == 1 else \"negative\",\n","                    \"score\": float(prob[label])\n","                }\n","                for label, prob in zip(labels, probs)\n","            ])\n","        except Exception as e:\n","            print(f\"Lỗi batch {i}: {e}\")\n","            for _ in batch_texts:\n","                results.append({\"label\": \"error\", \"score\": 0.0})\n","\n","    return results\n"],"metadata":{"id":"Tlw5BWDN5VTZ","executionInfo":{"status":"ok","timestamp":1745911692384,"user_tz":-420,"elapsed":3,"user":{"displayName":"Long Nguyen","userId":"14589141717299064407"}}},"execution_count":11,"outputs":[]},{"cell_type":"code","source":["# 5. Chạy sentiment\n","results = predict_sentiment_batch(texts, batch_size=512)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":373},"id":"xbrfSolo5VVU","executionInfo":{"status":"error","timestamp":1745917692605,"user_tz":-420,"elapsed":6000210,"user":{"displayName":"Long Nguyen","userId":"14589141717299064407"}},"outputId":"1e913618-fb32-40cc-e2cf-a9ec74b39cc4"},"execution_count":12,"outputs":[{"output_type":"stream","name":"stderr","text":["Predicting Sentiment: 100%|██████████| 4536/4536 [1:39:59<00:00,  1.32s/it]\n"